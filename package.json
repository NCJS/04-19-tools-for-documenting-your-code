{
  "name": "jsdoc-talk-ncjs",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "env": "^0.0.2",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.5.5",
    "lodash": "^4.17.5",
    "simple-statistics": "^5.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "nodemon": "^1.17.3"
  },
  "scripts": {
    "babel-node": "babel-node --presets=env",
    "start": "nodemon --exec npm run  babel-node ./index.js",
    "create-docs":
      "rm -rf ./docs && jsdoc --configure jsdoc.config.json --destination ./docs ./README.md"
  },
  "babel": {
    "presets": ["env"]
  }
}
